from tkinter import *

root = Tk()

e = Entry(root, width=50)
e.pack ()
e.insert(0, "What's your name:")

def myClick():
    hello = "You look lovely today, " + e.get()
    myLabel = Label(root, text=hello)
    myLabel.pack()

myButton = Button(root, text= "Click", command= myClick)
myButton.pack()

root.mainloop()
